---
import Layout from "../layouts/Layout.astro";

import Img404 from "../assets/404.png";
import HeaderMovil from "../components/HeaderMovil.astro";
import HeaderPc from "../components/HeaderPc.astro";
import Footer from "../components/Footer.astro";
---
<Layout>
    <HeaderMovil />
    <HeaderPc />
    <main class="flex flex-col items-center justify-center">
        <div class="flex flex-col items-center mt-16 ">

            <img src={Img404.src} alt="404 Not Found" class="w-1/2 max-w-sm mt-20" loading="lazy" />
            <p class="font-montserrat text-26px text-negroClaro font-medium">¡UPS! Navegaste mal</p>
            <p class="font-montserrat text-26px text-negroClaro font-bold">No se encontró la página.</p>

        </div>
    </main>
    <Footer />
</Layout>